<% if(posts) {posts.forEach(function(post, index) { %>
<div class="row" bgcolor="black">
    <div class ="panel panel-default" style="width:500px; margin-left:auto;margin-right:auto;">
        <div class="panel-heading">
            <p><a href="/u/<%= post.user %>"><%= post.user %></a> 发表于<%= post.time.toISOString().replace(/T/, ' ').replace(/\..+/, ' ') %></p>
        </div>
        <div class="panel-body">
            <p><%= post.post %></p>
        </div>
    </div>
</div>
<%})}%>